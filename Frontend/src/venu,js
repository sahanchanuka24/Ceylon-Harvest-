import React, { useState, useEffect } from 'react';

function App() {
  const [seconds, setSeconds] = useState(0);

  useEffect(() => {
    const interval = setInterval(() => {
      setSeconds(prev => prev + 1);
    }, 1000);
    return () => clearInterval(interval); // Clean up
  }, []);

  return (
    <div>
      <h2>Timer: {seconds}s</h2>
    </div>
  );
}

export default App;
