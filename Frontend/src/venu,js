import React from 'react';

// Child component
function Greeting(props) {
  return <h2>Hello, {props.name}!</h2>;
}

// Parent component
function App() {
  return (
    <div>
      <Greeting name="Thidas" />
      <Greeting name="Ayesha" />
      <Greeting name="Kasun" />
    </div>
  );
}

export default App;
